<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4A4A4A"
    tools:context=".persona">

    <EditText
        android:id="@+id/DNI"
        android:layout_width="240dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="DNI"
        android:inputType="number"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#4588BC"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#4588BC"
        android:textCursorDrawable="@color/common_google_signin_btn_text_dark_default"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/NOMBRE"
        app:layout_constraintTop_toBottomOf="@+id/NOMBRE" />

    <EditText
        android:id="@+id/NOMBRE"
        android:layout_width="240dp"
        android:layout_height="44dp"
        android:cursorVisible="true"
        android:ems="10"
        android:hint="NOMBRE"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#4588BC"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#4588BC"
        android:textCursorDrawable="@color/common_google_signin_btn_text_dark_default"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/APELLIDO"
        app:layout_constraintTop_toBottomOf="@+id/APELLIDO" />

    <EditText
        android:id="@+id/APELLIDO"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="APELLIDO"
        android:inputType="textPersonName"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#4588BC"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#4588BC"
        android:textCursorDrawable="@color/common_google_signin_btn_text_dark_default"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="false"
        android:onClick="Fecha"
        android:text="DD-MM-AAAA"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#4588BC"
        android:textColorLink="#4588BC"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/DNI"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewfechanac"
        app:layout_constraintTop_toBottomOf="@+id/textViewfechanac" />

    <TextView
        android:id="@+id/textViewfechanac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="FECHA DE NACIMIENTO"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/DNI"
        app:layout_constraintTop_toBottomOf="@+id/DNI" />

    <TextView
        android:id="@+id/textViewfechanac2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="EFECTOR DE SALUD"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/DNI"
        app:layout_constraintTop_toBottomOf="@+id/fecha" />

    <Button
        android:id="@+id/RIESGO"
        android:layout_width="125dp"
        android:layout_height="90dp"
        android:layout_marginTop="2dp"
        android:background="#4588BC"
        android:onClick="TipoFactores"
        android:text="FACTORES DE RIESGO"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/VACUNA"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        app:layout_constraintVertical_bias="0.036" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="63dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usuario" />

    <Button
        android:id="@+id/VACUNA"
        android:layout_width="125dp"
        android:layout_height="90dp"
        android:background="#4588BC"
        android:onClick="TiposVacuna"
        android:text="VACUNAS"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/divider2"
        app:layout_constraintTop_toTopOf="@+id/RIESGO" />

    <Button
        android:id="@+id/OBSERVACION"
        android:layout_width="125dp"
        android:layout_height="90dp"
        android:layout_marginTop="2dp"
        android:background="#4588BC"
        android:onClick="observaciones"
        android:text="NOTAS"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/divider2"
        app:layout_constraintTop_toBottomOf="@+id/VACUNA" />

    <Button
        android:id="@+id/OBSERVACION2"
        android:layout_width="290dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:background="#8BC34A"
        android:onClick="GuardarPersona"
        android:text="GUARDAR PERSONA"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/OBSERVACION"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="@+id/CONTACTO"
        app:layout_constraintTop_toBottomOf="@+id/OBSERVACION" />

    <Button
        android:id="@+id/CONTACTO"
        android:layout_width="125dp"
        android:layout_height="90dp"
        android:background="#4588BC"
        android:onClick="Contactos"
        android:text="CONTACTO"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/OBSERVACION"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/RIESGO"
        app:layout_constraintTop_toTopOf="@+id/OBSERVACION" />

    <View
        android:id="@+id/divider2"
        android:layout_width="290dp"
        android:layout_height="1dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/efector" />

    <View
        android:id="@+id/divider6"
        android:layout_width="1dp"
        android:layout_height="65dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/divider8"
        app:layout_constraintEnd_toStartOf="@+id/NOMBRE"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49" />

    <View
        android:id="@+id/divider8"
        android:layout_width="1dp"
        android:layout_height="150dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/divider2"
        app:layout_constraintEnd_toEndOf="@+id/divider6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/divider6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.861" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="27dp"
        android:layout_height="53dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/dni" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="32dp"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/calendario" />

    <Spinner
        android:id="@+id/efector"
        android:layout_width="242dp"
        android:layout_height="40dp"
        android:background="#4588BC"
        app:layout_constraintStart_toStartOf="@+id/textViewfechanac2"
        app:layout_constraintTop_toBottomOf="@+id/textViewfechanac2" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="33dp"
        android:layout_height="26dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/hospital" />


</androidx.constraintlayout.widget.ConstraintLayout>